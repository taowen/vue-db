{
  "name": "vue-db",
  "version": "0.0.2",
  "description": "vue data binding",
  "main": "dist/cjs/index.js",
  "browser": "src/index.ts",
  "types": "src/index.ts",
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "typescript": "^4.5.5"
  },
  "files": ["dist/cjs/*.js", "dist/esm/*.js", "dist/esm/*.d.ts"],
  "publishConfig": {
    "directory": "dist",
    "main": "cjs/index.js",
    "browser": "esm/index.js",
    "types": "esm/index.d.ts"
  },
  "scripts": {
    "build": "tsc",
    "dist": "tsc && tsc -p tsconfig-esm.json && cp ../../README.md dist && cp ../../LICENSE dist"
  }
}